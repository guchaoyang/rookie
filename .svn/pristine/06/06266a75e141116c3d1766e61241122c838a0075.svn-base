#import <UIKit/UIKit.h>

@interface MyCollectionViewCell : UICollectionViewCell
///商品图片
@property(nonatomic, strong)UIImageView *goodsPic;
///券后价图片
@property(nonatomic, strong)UIImageView *couponPriceImage;
///优惠券背景图
@property(nonatomic, strong)UIImageView *couponImage;

@property(nonatomic, strong)UILabel     *goodsName;

@property(nonatomic, strong)UILabel     *goodsPrice;
///已售
@property(nonatomic, strong)UILabel     *soldLabel;

///券后价格
@property(nonatomic, strong)UILabel     *couponPrice;
///优惠券金额
@property(nonatomic, strong)UILabel     *couponLabel;
///过期商品图片透明遮罩
@property(nonatomic, strong)UIView      *imageMatte;
///失效商品长条文字
@property(nonatomic, strong)UILabel     *invalidLabel;
///失效灰色文字标签
@property(nonatomic, strong)UILabel     *invalidTags;

@end
